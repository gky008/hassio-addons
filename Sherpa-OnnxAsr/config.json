{
  "name": "Sherpa-OnnxAsr",
  "version": "paraformer-2024-7-5",
  "slug": "Sherpa-OnnxAsr",
  "description": "本地离线语音模型，占用1GB",
  "url": "https://github.com/yaming116",
  "arch": ["amd64", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "webui": "http://[HOST]:[PORT:5001]",
  "image": "docker.awsl9527.cn/yaming116/sherpa-onnx-asr",
  "ports": {
    "5001/tcp": 5001
  },
  "map": {
    "config": {
      "path": "/data",
      "readonly": false
    }
  },
  "environment": {},
  "options": {
    "model_path": {
      "description": "模型文件路径(如果不不明白，请勿修改）",
      "type": "string",
      "default": "/data/model.onnx"
    },
    "sample_rate": {
      "description": "音频采样率",
      "type": "integer",
      "default": 16000
    },
    "language": {
      "description": "语言选择",
      "type": "string",
      "default": "zh-CN"
    }
  },
  "schema": {
    "model_path": "str",
    "sample_rate": "int",
    "language": "str"
  }
}

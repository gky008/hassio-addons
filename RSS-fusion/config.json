{
  "name": "Fusion-RSS",
  "version": "0.8.2",
  "slug": "fusion",
  "description": "A lightweight, self-hosted friendly RSS aggregator and reader 一个轻量级的、自托管的友好型 RSS 聚合器和阅读器",
  "url": "https://github.com/0x2E/fusion",
  "arch": ["amd64", "aarch64", "arm64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "webui": "http://[HOST]:[PORT:8080]",
  "image": "rook1e404/fusion",
  "ports": {
    "8080/tcp": 8080
  },
  "map": {
    "config": {
      "path": "/data",
      "readonly": false
    }
  },
  "environment": {
    "PASSWORD": "123456"
  }
}
